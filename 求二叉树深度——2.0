class Solution {
public:
    int TreeDepth(TreeNode* pRoot)
    {
        if(NULL == pRoot)
            return 0;
        int lefthight = TreeDepth(pRoot->left);
        int righthight = TreeDepth(pRoot->right);
        return lefthight > righthight ? lefthight+1 : righthight+1;
    }
};

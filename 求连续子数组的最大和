#include <iostream>
using namespace std;


int MaxSumOfSubArray(int* arr, int size)
{
    if(NULL == arr || size < 1)
        return 0;
    int sum = 0;
    int maxsum = 0;
    int max = arr[0];
    for(int i = 0; i < size; ++i)
    {
        sum += arr[i];
        if(sum < 0)
            sum = 0;
        if(arr[i] > max)
            max = arr[i];
        if(sum > maxsum)
            maxsum = sum;
    }
    if(max < 0)
        return max;
    return maxsum;
}

int main()
{
    int n = 0;
    cin >> n;
    int arr[n];
    for(int i = 0; i < n; ++i)
        cin >> arr[i];
    cout << MaxSumOfSubArray(arr, n) << endl;
    
    return 0;
}
